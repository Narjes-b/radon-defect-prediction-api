#!/home/stefadp/PycharmProjects/radon-defect-prediction-api/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'repository-miner==0.2.4','console_scripts','repository-miner'
__requires__ = 'repository-miner==0.2.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('repository-miner==0.2.4', 'console_scripts', 'repository-miner')()
    )
